% The default config which gives a basic fast simulation. 
% This config is if no other configfile is called

RUN: verify
 	NR_STRIDES: 14
	STRIDE_LEN: 10
	GROWTH: 1.5
	SAMPLES: 30

	TYPE: saw
	VERBOSE: true
	OUTFILE: ../data/default_data.dna

RUN: visualize
	OUTFILE: ../data/default_visualize_data.dna
	SIZE: 10

	VERBOSE: false
