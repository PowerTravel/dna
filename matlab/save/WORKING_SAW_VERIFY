% The default config which gives a basic fast simulation. 
% This config is if no other configfile is called


RUN: verify
	SIZE: 1000
 	STRIDES: 100
	EXP: false
	SAMPLES: 100

	TYPE: saw
	VERBOSE: false
	OUTFILE: ../data/Verify_Default.dna

RUN: visualize
	OUTFILE: ../data/Visualize_Default.dna
	SIZE: 10

	VERBOSE: false



